  "scripts": {
    "build": "tsc --noEmit && esbuild --bundle --platform=node --target=node18 --outdir=dist src/index.ts",
    "start": "NODE_ENV=development tsx --watch src/index.ts",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,md,html,css}\"",
    "lint": "tsc --noEmit && eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings=0",
    "reproduce": "npx tsx scripts/vote-products.ts",
    "generate": "pnpm generate:types && pnpm generate:migrations && pnpm format",
    "generate:types": "graphql-codegen",
    "generate:migrations": "drizzle-kit generate:pg",
    "generate:schema": "rover graph introspect http://localhost:3000/graphql --output schema.graphql"
  },
